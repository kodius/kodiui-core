#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@docusaurus+core@2.4.0_eslint@8.36.0_react-dom@17.0.2_react@17.0.2_typescript@4.9.5/node_modules/@docusaurus/core/bin/docusaurus.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@docusaurus+core@2.4.0_eslint@8.36.0_react-dom@17.0.2_react@17.0.2_typescript@4.9.5/node_modules/@docusaurus/core/bin/docusaurus.mjs" "$@"
fi
