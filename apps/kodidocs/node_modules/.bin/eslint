#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/bin/eslint.js" "$@"
fi
